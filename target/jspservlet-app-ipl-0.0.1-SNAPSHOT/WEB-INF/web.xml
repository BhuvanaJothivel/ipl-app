<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SemiFinals1Servlet</servlet-name>
  	<display-name>SemiFinals1Servlet</display-name>
  	<description></description>
  	<servlet-class>controller.SemiFinals1Servlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SemiFinals2Servlet</servlet-name>
  	<display-name>SemiFinals2Servlet</display-name>
  	<description></description>
  	<servlet-class>controller.SemiFinals2Servlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>FinalsServlet</servlet-name>
  	<display-name>FinalsServlet</display-name>
  	<description></description>
  	<servlet-class>controller.FinalsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>default</servlet-name>
  	<url-pattern>*.css</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  <servlet-name>default</servlet-name>
  	<url-pattern>*.svg</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SemiFinals1Servlet</servlet-name>
  	<url-pattern>/qualifier1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SemiFinals2Servlet</servlet-name>
  	<url-pattern>/qualifier2</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>FinalsServlet</servlet-name>
  	<url-pattern>/finals</url-pattern>
  </servlet-mapping>
</web-app>
